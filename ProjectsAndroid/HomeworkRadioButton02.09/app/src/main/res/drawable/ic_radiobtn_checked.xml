<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <shape android:shape="oval">
            <size
                android:width="80dp"
                android:height="80dp" />
            <solid android:color="#29b6f6" />

            <stroke
                android:width="18dp"
                android:color="@android:color/white"></stroke>
        </shape>
    </item>
    <item
        android:left="5dp"
        android:right="5dp">
        <shape
            android:innerRadius="35dp"
            android:shape="ring"
            android:thickness="5dp"
            android:useLevel="false">
            <solid android:color="#29b6f6" />
            <size
                android:width="80dp"
                android:height="80dp" />

        </shape>
    </item>


</layer-list>